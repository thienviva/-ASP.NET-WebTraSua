@model IEnumerable<WebQuanLyQuanTraSua.Models.QUANLY>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/AdminLayout.cshtml";
}

<h2>Danh Sách Quản Lý</h2>

<p>
    <div class="btn btn-default">
        @Html.ActionLink("Thêm mới", "Create")
    </div>
        <div class="btn btn-default">
            @Html.ActionLink("Tài khoản quản lý", "Index", "TAIKHOANQLs")
        </div>
</p>
<table class="table">
    <tr>
        <th>
            Họ và tên:
        </th>
        <th>
            Email:
        </th>
        <th>
            Tuổi:
        </th>
        <th>
            Địa chỉ:
        </th>
        <th>
            Số điện thoại:
        </th>
        <th>
            Lương:
        </th>
        <th>
            Hình ảnh:
        </th>
        <th>
            Tài khoản:
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TenQL)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Tuoi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DiaChi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SDT)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Luong)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.HinhAnh)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TAIKHOANQL.UserName)
    </td>
    <td>
        @Html.ActionLink("Chỉnh sửa", "Edit", new { id = item.MaQL }) |
        @Html.ActionLink("Chi tiết", "Details", new { id = item.MaQL }) |
        @Html.ActionLink("Xóa", "Delete", new { id = item.MaQL })
    </td>
</tr>
}

</table>
